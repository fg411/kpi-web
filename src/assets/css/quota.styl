.quota_page
  background-color transparent
  .blank_box
    height:3.5rem
  .quota_head
    height 3.5rem
    width 100%
    position: fixed
    z-index 100
    .user_info_box
      padding: 0.3rem;
      color: white;
      //position: absolute;
      //position: fixed;
      width: 100%;
      height: 2.5rem;
      z-index: 2;
      background-color: rgba(67, 120, 190, 1);
    .tab
      position: sticky
      top: 3.5rem
      z-index 2
      background-color rgba(67, 120, 190, 1)
      .md-tab-bar-list
        background-color #FFF

  .quota_content
    //padding-top: 2.5rem;
    height: calc(100vh - 3.5rem);
    .tab
      position: sticky;
      top: 3.5rem
      z-index 2
      .md-tab-bar-list
        background-color #FFF
    .md-swiper
      background-color #F3F6F9
      height calc(100vh - 3.39rem)
      padding-bottom 0.1rem

    .quota_group
      background-color #FFF;
      margin-top 0.2rem;

      .group_title
        border-bottom: solid 1px #e3e3e3;
        padding: 0.2rem 0.3rem;
        display: flex;
        height: 0.88rem;
        align-items: center;

        .group_title_left
          flex-grow: 2;
          font-size: 0.28rem;
          color: #333333;
          //letter-spacing: 0;
          position: relative;
          height 0.48rem
          line-height 0.48rem

          span.icon{
            width: 0.36rem;
            height: 0.36rem;
            background-size: 0.36rem 0.36rem;
            display: inline-block;
            vertical-align: sub;
          }

          .idea_practice { // 集团理念实现类
            background: url("../img/idea_practice.png") no-repeat
          }
          .professionalism{ // 个人职业精神类
            background: url("../img/professionalism.png") no-repeat
          }
          .knowledge{ // 专业知识类
            background: url("../img/knowledge.png") no-repeat
          }
          .sharing{ // 专业分享类
            background: url("../img/sharing.png") no-repeat
          }
          .productDesign // 产品设计类
            background url("../img/product_design.png") no-repeat

          .performanceAchieved // 业绩实现类
            background url("../img/performance_achieved.png") no-repeat

          .revenueQuality // 创收质量类
            background url("../img/revenue_quality.png") no-repeat

          .customerManage // 客户管理类
            background url("../img/customer_manage.png") no-repeat

          .businessProcessDrive // 业务流程推动类
            background url("../img/business_process_drive.png") no-repeat

          .professionalApplication // 专业应用类
            background url("../img/professional_application.png") no-repeat

          .marketingManage // 支持营销管理类
            background url("../img/marketing_manage.png") no-repeat

          .zhanyeSupport // 适用于展业支持类营销管理类人员
            background url("../img/zhanye_support.png") no-repeat

          .productContribution // 产品贡献类
            background url("../img/product_contribution.png") no-repeat

          .productPromotion // 产品推动类
            background url("../img/product_promotion.png") no-repeat

          .productMarketing // 产品推广类
            background url("../img/product_marketing.png") no-repeat

          .overallSupport // 适用于统筹支持类营销管理类人员
            background url("../img/overall_support.png") no-repeat

        .group_title_right{
          position: relative;
          color: #D9D9D9;
          font-size .2rem
          span {
            font-size: .33rem;
            color: #4378be
          }
          span:before{
            content: '';
            width: 0.36rem;
            height: 0.36rem;
            background-size: 0.36rem 0.36rem;
            background url("../img/rank.png")
            background-repeat round
            position: absolute;
            left: -0.5rem;
            top: -0.02rem
          }
        }

      .group_content
        padding: 0.1rem 0.3rem;


